    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "jest": "^22.1.1",
    "react-test-renderer": "^16.2.0",
    "webpack-dev-server": "^2.9.5"
  },
  "proxy": "http://localhost:3003/"
}
